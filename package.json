{
  "name": "betterbeing-web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "node node_modules/next/dist/bin/next dev",
    "build": "node scripts/validate-env.mjs && node node_modules/next/dist/bin/next build",
    "validate:env": "node scripts/validate-env.mjs",
    "prestart": "node -e \"const fs=require('fs'); if(!fs.existsSync('.next')){console.error('❌ Missing .next build output. Run \\\"pnpm run build\\\" first.'); process.exit(1)}\"",
    "start": "node node_modules/next/dist/bin/next start",
    "lint": "eslint .",
    "dev:server": "cd server && pnpm run dev",
    "dev:all": "concurrently -n WEB,API -c green,cyan \"pnpm run dev\" \"pnpm run dev:server\"",
    "build:server": "cd server && pnpm install --prod",
    "build:frontend": "node node_modules/next/dist/bin/next build",
    "build:dev": "node node_modules/next/dist/bin/next build",
    "build:analyze": "set ANALYZE=true&& node node_modules/next/dist/bin/next build",
    "vercel-build": "pnpm run build:server && pnpm run build:frontend",
    "build:vercel": "pnpm run vercel-build",
    "preview": "node node_modules/next/dist/bin/next start",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx lib/db/seed.ts",
    "db:setup": "pnpm run db:generate && pnpm run db:migrate && pnpm run db:seed",
    "db:reset": "drizzle-kit drop && pnpm run db:setup",
    "db:init": "cd server && node src/config/init-db.js",
    "db:seed:legacy": "cd server && node --input-type=module -e \"import seedDatabase from './src/config/seed.js'; seedDatabase().then(() => console.log('✅ Seeding completed!')).catch(console.error)\"",
    "install:all": "pnpm install && cd server && pnpm install",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:all": "pnpm run optimize:images && pnpm run build:frontend",
    "build:optimized": "pnpm run optimize:images && set ANALYZE=true&& node node_modules/next/dist/bin/next build",
    "performance:audit": "lighthouse http://localhost:3000 --output=json --output-path=./performance-audit.json",
    "playwright:install": "pnpm exec playwright install --with-deps",
    "test:visual": "playwright test --project=visual-regression",
    "test:visual:headed": "playwright test --project=visual-regression --headed",
    "test:design-capture": "playwright test --project=design-capture --headed",
    "test:design-validation": "pnpm run test:design-capture && pnpm run test:visual && pnpm run audit:design-system",
    "test:component-visual": "playwright test --grep component-visual",
    "test:mobile": "playwright test --project=mobile",
    "test:desktop": "playwright test --project=desktop",
    "audit:design-system": "node scripts/design-system-audit.js",
    "update:visual-baselines": "playwright test --project=visual-regression --update-snapshots",
    "ai:design-iteration": "node scripts/ai-design-iteration.js",
    "data:agents": "node scripts/generate-locations-from-excel.mjs",
    "audit:code": "tsx scripts/audit-code-quality.ts",
    "quality:check": "pnpm run lint && pnpm run audit:code && pnpm run test:run",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
